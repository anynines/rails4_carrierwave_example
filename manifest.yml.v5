---
applications:
- name: your_app_name
  memory: 512M
  instances: 1
  host: your_app_name
  domain: de.a9sapp.eu
  path: .
  buildpack: https://github.com/cloudfoundry/heroku-buildpack-ruby.git
  services:
    postgresql-your_app_name:
      label: postgresql
      provider: core
      version: '9.1'
      plan: Pluto-free
    swift-your_app_name:
      label: swift
      provider: core
      version: '1.0'
      plan: free
